{
  "name": "cadop-service",
  "version": "1.0.0",
  "description": "Custodian-Assisted DID Onboarding Protocol Service",
  "main": "dist/index.js",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "vite build",
    "build:backend": "node scripts/build.js",
    "build": "npm run build:frontend && npm run build:backend",
    "watch": "node scripts/build.js --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/utils/migrate.ts migrate",
    "db:seed": "tsx src/utils/migrate.ts seed",
    "db:reset": "tsx src/utils/migrate.ts reset",
    "db:status": "tsx src/utils/migrate.ts status",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@roochnetwork/rooch-sdk": "^0.3.4",
    "@simplewebauthn/browser": "^8.3.4",
    "@simplewebauthn/server": "^8.3.5",
    "@simplewebauthn/types": "^12.0.0",
    "@supabase/supabase-js": "^2.49.8",
    "antd": "^5.25.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.309.0",
    "next-themes": "^0.2.1",
    "node-jose": "^2.2.0",
    "nuwa-identity-kit": "file:../../../nuwa-kit/typescript/packages/nuwa-identity-kit",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-twitter": "^1.0.39",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "esbuild-node-externals": "^1.12.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.1.4",
    "typescript": "^5.3.3",
    "vite": "^4.5.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "did",
    "identity",
    "oidc",
    "web3",
    "authentication",
    "rooch"
  ],
  "author": "Nuwa Network",
  "license": "MIT"
}
