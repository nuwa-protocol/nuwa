{
  "name": "@nuwa-ai/ui-kit",
  "version": "0.5.3",
  "description": "Nuwa UI Kit for TypeScript/React",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "penpal": "^7.0.4"
  },
  "devDependencies": {
    "@types/react": "^19.1.11",
    "react": "^19.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.2.3",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "nuwa",
    "capui",
    "react",
    "sdk",
    "iframe"
  ],
  "author": "Nuwa AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuwa-protocol/nuwa.git",
    "directory": "nuwa-kit/typescript/packages/capui-kit"
  },
  "bugs": {
    "url": "https://github.com/nuwa-protocol/nuwa/issues"
  },
  "homepage": "https://github.com/nuwa-protocol/nuwa/tree/main/nuwa-kit/typescript/packages/ui-kit#readme",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}