{
  "name": "@nuwa-ai/payment-kit",
  "version": "0.6.5",
  "description": "SDK for NIP-4 Unidirectional Payment Channels on Rooch and other ledgers",
  "type": "module",
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "browser": "./dist/browser/index.js",
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./http": {
      "types": "./dist/node/integrations/http/index.d.ts",
      "browser": "./dist/browser/integrations/http/index.js",
      "import": "./dist/node/integrations/http/index.js",
      "require": "./dist/node/integrations/http/index.cjs"
    },
    "./express": {
      "types": "./dist/node/transport/express/index.d.ts",
      "import": "./dist/node/transport/express/index.js",
      "require": "./dist/node/transport/express/index.cjs"
    },
    "./storage/sql": {
      "types": "./dist/node/storage/sql/index.d.ts",
      "import": "./dist/node/storage/sql/index.js",
      "require": "./dist/node/storage/sql/index.cjs"
    },
    "./storage/indexeddb": {
      "types": "./dist/node/empty.d.ts",
      "browser": "./dist/browser/storage/indexeddb/index.js",
      "import": "./dist/node/empty.js",
      "require": "./dist/node/empty.cjs"
    },
    "./mcp": {
      "types": "./dist/node/transport/mcp/index.d.ts",
      "import": "./dist/node/transport/mcp/index.js",
      "require": "./dist/node/transport/mcp/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json --coverage",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json --watch",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json --testPathPattern=integration",
    "test:mcp": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json --testPathPattern=mcp",
    "test:mcp:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.json --testPathPattern=mcp --watch",
    "test:e2e": "jest --config=jest.config.e2e.ts --runInBand",
    "test:e2e:http": "NODE_OPTIONS='--experimental-vm-modules' ROOCH_NODE_URL=http://localhost:6767 PAYMENT_E2E=1 jest --config=jest.config.e2e.ts --runInBand --testPathPattern=HttpPaymentKit",
    "test:e2e:local": "pnpm test:e2e:http && pnpm test:e2e:mcp",
    "test:e2e:mcp": "NODE_OPTIONS='--experimental-vm-modules' ROOCH_NODE_URL=http://localhost:6767 PAYMENT_E2E=1 jest --config=jest.config.e2e.ts --runInBand --testPathPattern=McpPaymentKit",
    "test:e2e:verify": "ts-node --esm test/e2e/verify-setup.ts",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"{src,test,docs}/**/*.{ts,tsx,js,jsx,md,mdx,json,yaml,yml}\" --list-different",
    "format:check": "prettier --check \"{src,test,docs}/**/*.{ts,tsx,js,jsx,md,mdx,json,yaml,yml}\"",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "payment-channel",
    "nip-4",
    "rooch",
    "blockchain",
    "micropayments",
    "subrav",
    "nuwa",
    "did"
  ],
  "author": "Nuwa Community",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuwa-protocol/nuwa.git",
    "directory": "nuwa-kit/typescript/packages/payment-kit"
  },
  "bugs": {
    "url": "https://github.com/nuwa-protocol/nuwa/issues"
  },
  "homepage": "https://github.com/nuwa-protocol/nuwa/tree/main/nuwa-kit/typescript/packages/payment-kit#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/on-headers": "^1.0.4",
    "@types/path-to-regexp": "^1.7.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.0",
    "tsup": "^8.2.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@nuwa-ai/identity-kit": "workspace:*",
    "@roochnetwork/rooch-sdk": "^0.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "^8.15.4",
    "ai": "^5.0.60",
    "fastmcp": "^3.5.0",
    "js-yaml": "^4.1.0",
    "lossless-json": "^4.1.1",
    "mcp-proxy": "^5.5.4",
    "on-headers": "^1.1.0",
    "path-to-regexp": "^6.3.0",
    "pg": "^8.16.3",
    "zod": "^3.25.3",
    "zod-to-json-schema": "^3.24.6"
  },
  "sideEffects": false,
  "peerDependencies": {
    "express": ">=4",
    "pg": ">=8"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "tsup": {
    "clean": true,
    "sourcemap": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true,
    "entry": {
      "browser/index": "src/index.browser.ts",
      "node/index": "src/index.node.ts",
      "browser/integrations/http/index": "src/integrations/http/index.ts",
      "node/integrations/http/index": "src/integrations/http/index.ts",
      "node/transport/express/index": "src/transport/express/index.ts",
      "node/transport/mcp/index": "src/transport/mcp/index.ts",
      "node/storage/sql/index": "src/storage/sql/index.ts",
      "browser/storage/indexeddb/index": "src/storage/indexeddb/index.ts",
      "node/empty": "src/node/empty.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "dist",
    "platform": "neutral",
    "target": "es2020"
  }
}