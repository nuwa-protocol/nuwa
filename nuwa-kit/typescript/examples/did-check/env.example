# Network Configuration
ROOCH_NETWORK=main

# Optional: Custom Rooch RPC URL. If unset, the VDR uses the default RPC URL for the network specified in ROOCH_NETWORK.
# Example: ROOCH_NODE_URL=https://seed.rooch.network
# ROOCH_NODE_URL=

# Server Configuration
PORT=3004

# DEBUG mode controls whether error stack traces are exposed in HTTP responses.
# Keep this disabled in production; enable only in tightly controlled development environments.
DEBUG=false
