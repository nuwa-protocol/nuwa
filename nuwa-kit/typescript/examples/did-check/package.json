{
    "name": "@nuwa-ai/did-check-example",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "Minimal did-check service using identity-kit for DID authentication on Rooch mainnet",
    "scripts": {
        "build": "tsc",
        "dev:server": "tsx src/server.ts",
        "start:server": "node dist/server.js",
        "clean": "rm -rf dist",
        "format": "prettier --write \"{src,tests}/**/*.{ts,tsx,js,jsx,json,md,mdx,yaml,yml}\"",
        "format:check": "prettier --check \"{src,tests}/**/*.{ts,tsx,js,jsx,json,md,mdx,yaml,yml}\""
    },
    "dependencies": {
        "@nuwa-ai/identity-kit": "^0.6.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.3",
        "@types/express": "^4.17.21",
        "@types/node": "^20.0.0",
        "prettier": "^3.2.5",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0"
    }
}
